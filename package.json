{
    "name": "edelweiss-dojo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node app.js"
    },
    "dependencies": {
        "express": "3.2.0",
        "jade": "*",
        "async" : "*",
        "underscore" : "*"
    },
    "devDependencies" : {
        "mocha" : "*",
        "superagent" : "*",
        "should" : "*",
        "cheerio" : "*"
    },
    "publicConfig": {
        "http_port": 3000,
        "request_timeout": 10000,
        "session_secret": "MySuperSecretKey",
        "log_requests": false,
        "test": false,
        "db" : {
            "name" : "edelweissdojo",
            "host" : "localhost",
            "port" : 27017,
            "options" : {
                "auto_reconnect" : true,
                "poolSize" : 4,
                "socketOptions" : { "encoding" : "utf-8" }
            }
        }
    }
}